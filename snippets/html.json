{
  "select": {
    "prefix": "ui-select",
    "body": [
        "ui-select theme=\"bootstrap\">",
        "\t<ui-select-match placeholder=\"${3:DefaulText}\">",
        "\t</ui-select-match>",
        "\t<ui-select-choices repeat=\"${1:element} in ${2:optionsList}\">",
        "\t</ui-select-choices>",
        "</ui-select>"
    ],
    "description": "vanilla AngularJS ui-select"
  }
}
